{
	"name": "portfolio",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"lint:fix": "eslint --fix .",
		"prettier": "prettier --write .",
		"type:check": "tsc --outDir out --noEmit",
		"prepare": "husky install",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:e2e": "playwright test",
		"test:e2e:head": "playwright test --headed",
		"prisma:gen": "prisma generate",
		"prisma:mig:dev": "prisma migrate dev",
		"prisma:std": "prisma studio",
		"prisma:push": "prisma db push",
		"prisma:pull": "prisma db pull"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"prisma": {
		"schema": "src/prisma/schema.prisma"
	},
	"dependencies": {
		"@floating-ui/react-dom": "0.4.3",
		"@next-auth/prisma-adapter": "1.0.1",
		"@prisma/client": "3.8.1",
		"canvas-confetti": "1.4.0",
		"cobe": "0.4.1",
		"framer-motion": "6.2.1",
		"next": "12.0.8",
		"next-auth": "4.1.2",
		"next-seo": "4.29.0",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-icons": "4.3.1",
		"react-intersection-observer": "8.33.1",
		"react-use": "17.3.2",
		"zustand": "3.6.9"
	},
	"devDependencies": {
		"@next/bundle-analyzer": "12.0.8",
		"@playwright/test": "1.18.0",
		"@tailwindcss/typography": "0.5.0",
		"@testing-library/jest-dom": "5.16.1",
		"@testing-library/react": "12.1.2",
		"@types/canvas-confetti": "^1.4.2",
		"@types/node": "17.0.11",
		"@types/react": "17.0.38",
		"@typescript-eslint/eslint-plugin": "5.10.1",
		"autoprefixer": "10.4.2",
		"eslint": "8.7.0",
		"eslint-config-next": "12.0.8",
		"eslint-config-prettier": "8.3.0",
		"husky": "7.0.4",
		"jest": "27.4.7",
		"lint-staged": "12.3.1",
		"next-translate": "1.3.2",
		"postcss-import": "14.0.2",
		"prettier": "2.5.1",
		"prisma": "3.8.1",
		"tailwindcss": "3.0.16",
		"tailwindcss-theme-swapper": "0.4.1",
		"typescript": "4.5.5"
	}
